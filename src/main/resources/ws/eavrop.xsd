<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://inera.ws/fmu/admin/eavrop" targetNamespace="http://inera.ws/fmu/admin/eavrop" elementFormDefault="qualified">

	<!-- -->
	<xs:element name="skapaFmuEavropRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="landstingId" type="xs:int"/>
				<xs:element name="utredningType" type="tns:utredningType"/>
				<xs:element name="beskrivning" type="xs:string" minOccurs="0"/>
				<xs:element name="invanare" type="tns:invanare"/>
				<xs:element name="administrator" type="tns:person"/>
				<xs:element name="tolk" type="xs:boolean" minOccurs="0"/>
				<xs:element name="tolkSprak" type="xs:string" minOccurs="0"/>
				<xs:element name="utredningFokus" type="xs:string" minOccurs="0"/>
				<xs:element name="ytterligareInformation" type="xs:string" minOccurs="0"/>
				<xs:element name="tidigareUtredning" type="tns:tidigareUtredning" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="skapaFmuEavropResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="fmuResponse" type="tns:fmuResponse"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <!-- -->

    <!-- -->
	<xs:element name="skickatFmuHandlingarRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="handlingarSkickadeDateTime" type="xs:dateTime"/>
				<xs:element name="handlingarSkickadeAv" type="tns:person"/>
				<xs:element name="handlingar" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="skickatFmuHandlingarResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="begarKompletteringFmuHandlingRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="handlingBegardDateTime" type="xs:dateTime"/>
				<xs:element name="handlingBegardAv" type="tns:person"/>
				<xs:element name="handling" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="begarKompletteringFmuHandlingResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="svarBokningsavvikelseRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="bokningsId" type="tns:bokningsId"/>
				<xs:element name="svar" type="tns:bookingDeviationResponse"/>
				<xs:element name="svarDateTime" type="xs:dateTime"/>
				<xs:element name="notering" type="tns:notering" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="svarBokningsavvikelseResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="skickatFmuIntygRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="intygSkickatDateTime" type="xs:dateTime"/>
				<xs:element name="intygSkickatAv" type="tns:hoSPerson"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="skickatFmuIntygResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="begartFmuIntygKompletteringRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="intygKompletteringBegardDateTime" type="xs:dateTime"/>
				<xs:element name="intygKompletteringBegardAv" type="tns:person"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="begartFmuIntygKompletteringResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- Ta bort, samma som skickatFmuIntygRequest -->
	<xs:element name="skickatFmuIntygKompletteringRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="intygSkickatDateTime" type="xs:dateTime"/>
				<xs:element name="intygSkickatAv" type="tns:person"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="skickatFmuIntygKompletteringResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="accepteratFmuIntygRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="intygAccepteratDateTime" type="xs:dateTime"/>
				<xs:element name="intygAccepteratAv" type="tns:person"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="accepteratFmuIntygResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="accepteratFmuUtredningRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="utredningAccepteradDateTime" type="xs:dateTime"/>
				<xs:element name="utredningAccepteradAv" type="tns:person"/>
				<xs:element name="notering" type="tns:notering" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="accepteratFmuUtredningResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- -->

	<!-- -->
	<xs:element name="godkannandeErsattningFmuUtredningRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="arendeId" type="tns:arendeId"/>
				<xs:element name="godkand" type="xs:boolean"/>
				<xs:element name="utredningErsattningGodkandDateTime" type="xs:dateTime"/>
				<xs:element name="utredningErsattningGodkandAv" type="tns:person"/>
				<xs:element name="notering" type="tns:notering" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
    <xs:element name="godkannandeErsattningFmuUtredningResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="fmuResponse" type="tns:fmuResponse"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
	<!-- -->

    <xs:complexType name="fmuResponse">
        <xs:sequence>
            <xs:element name="arendeId" type="tns:arendeId"/>
            <xs:element name="statusCode" type="tns:statusCode"/>
            <xs:element name="errorMessage" type="tns:errorMessage" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
	<xs:complexType name="adress">
		<xs:sequence>
			<xs:element name="postadress" type="xs:string"/>
			<xs:element name="postnummer" type="xs:string"/>
			<xs:element name="stad" type="xs:string"/>
			<xs:element name="land" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="invanare">
		<xs:sequence>
			<xs:element name="personnummer" type="xs:string"/>
			<xs:element name="namn" type="tns:namn"/>
			<xs:element name="kon" type="tns:gender"/>
			<xs:element name="adress" type="tns:adress"/>
			<xs:element name="telefon" type="xs:string"/>
			<xs:element name="epost" type="tns:epostAdress"/>
			<xs:element name="sarskildaBehov" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="person">
		<xs:sequence>
			<xs:element name="namn" type="xs:string"/>
			<xs:element name="id" type="xs:string" minOccurs="0"/>
			<xs:element name="befattning" type="xs:string"/>
			<xs:element name="enhet" type="xs:string"/>
			<xs:element name="organisation" type="xs:string"/>
			<xs:element name="telefon" type="xs:string"/>
			<xs:element name="epost" type="tns:epostAdress"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="hoSPerson">
		<xs:sequence>
			<xs:element name="namn" type="xs:string"/>
			<xs:element name="hsaId" type="xs:string"/>
			<xs:element name="befattning" type="xs:string"/>
			<xs:element name="enhet" type="xs:string"/>
			<xs:element name="organisation" type="xs:string"/>
			<xs:element name="telefon" type="xs:string"/>
			<xs:element name="epost" type="tns:epostAdress"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="namn">
		<xs:sequence>
			<xs:element name="fornamn" type="xs:string"/>
			<xs:element name="mellannamn" type="xs:string" minOccurs="0"/>
			<xs:element name="efternamn" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tidigareUtredning">
		<xs:sequence>
			<xs:element name="utreddVid" type="xs:string"/>
			<xs:element name="sjukskrivandeenhet" type="xs:string"/>
			<xs:element name="sjukskrivenAv" type="tns:person"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="notering">
		<xs:sequence>
			<xs:element name="notering" type="xs:string"/>
			<xs:element name="noteradAv" type="tns:person"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="utredningType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="AFU"/>
			<xs:enumeration value="TMU"/>
			<xs:enumeration value="SLU"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="gender">
		<xs:restriction base="xs:string">
			<xs:enumeration value="MALE"/>
			<xs:enumeration value="FEMALE"/>
			<xs:enumeration value="OTHER"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bookingDeviationResponse">
		<xs:restriction base="xs:string">
			<xs:enumeration value="STOP"/>
			<xs:enumeration value="RESTART"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="epostAdress">
		<xs:restriction base="xs:string">
			<xs:pattern value="[^@]+@[^\.]+\..+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="arendeId">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="bokningsId">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="statusCode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="OK"/>
			<xs:enumeration value="INVALID_REQUEST"/>
			<xs:enumeration value="REQUEST_DENIED"/>
			<xs:enumeration value="UNKNOWN_ERROR"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="errorMessage">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
</xs:schema>
